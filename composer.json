{
    "name": "webeweb/adminbsb-material-design-bundle",
    "type": "symfony-bundle",
    "description": "Integrate AdminBSB Material Design theme with Symfony 2",
    "keywords": ["webeweb", "bundle", "symfony", "adminbsb", "material design", "theme"],
    "homepage": "http://github.com/webeweb/adminbsb-material-design-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "http://github.com/webeweb/"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^5.6|^7.0",
        "friendsofsymfony/user-bundle": "^1.3|^2.0",
        "symfony/framework-bundle": "^2.6|^3.0",
        "symfony/security-core": "^2.6|^3.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/bootstrap-bundle": "^1.0",
        "webeweb/core-library": "^2.0",
        "webeweb/jquery-datatables-bundle": "^1.0",
        "webeweb/syntaxhighlighter-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "symfony/expression-language": "^2.6|^3.0"
    },
    "suggest": {
        "webeweb/jquery-querybuilder-bundle": "*"
    },
    "support": {
        "issues": "http://github.com/webeweb/adminbsb-material-design-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\AdminBSBBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
