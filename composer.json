{
    "name": "webeweb/adminbsb-material-design-bundle",
    "type": "symfony-bundle",
    "description": "Integrate AdminBSB Material Design theme with Symfony 2 and more",
    "keywords": ["webeweb", "bundle", "symfony", "adminbsb", "material design", "theme"],
    "homepage": "https://github.com/webeweb/adminbsb-material-design-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "https://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "symfony/asset": "^2.7|^3.0|^4.0",
        "symfony/form": "^2.7|^3.0|^4.0",
        "symfony/framework-bundle": "^2.7|^3.0|^4.0",
        "symfony/security-core": "^2.7|^3.0|^4.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/bootstrap-bundle": "^1.7",
        "webeweb/core-library": "^5.0",
        "webeweb/jquery-datatables-bundle": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*|^7.0",
        "doctrine/doctrine-bundle": "^1.0",
        "friendsofsymfony/user-bundle": "^1.3|^2.0",
        "symfony/browser-kit": "^2.7|^3.0|^4.0",
        "symfony/monolog-bundle": "^2.7|^3.0|^4.0",
        "symfony/security-bundle": "^2.7|^3.0|^4.0",
        "symfony/swiftmailer-bundle": "^2.6|^3.0",
        "symfony/templating": "^2.7|^3.0|^4.0",
        "symfony/twig-bundle": "^2.7|^3.0|^4.0"
    },
    "support": {
        "issues": "https://github.com/webeweb/adminbsb-material-design-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\AdminBSBBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WBW\\Bundle\\AdminBSBBundle\\Tests\\": "Tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
